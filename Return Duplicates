public class Solution {
    // DO NOT MODIFY THE ARGUMENTS WITH "final" PREFIX. IT IS READ ONLY
    public int repeatedNumber(final int[] A) {
        HashSet<Integer>seen = new HashSet<>();
        for(int num : A){
            if(seen.contains(num)){
                return num;
            }
            seen.add(num);
        }
        return -1;
    }
}
